<div class="entry">
  <h1>{{meetup.name}}</h1>
  <div class="body">
    {{meetup.description}}
  </div>
</div>

<p>
  {{location.name}}
</p>

<h2>Members</h2>

{{#each user}}
  <section>
    <p>{{name}}</p>
  </section>
{{/each}}

<h2>Followers</h2>

{{#each followers}}
  <section>
    <p>{{name}}</p>
  </section>
{{/each}}

<h2>You may be interested in...</h2>

{{#each otherMeetups}}
  <section>
    <h1><a href="/meetups/{{_id}}">{{name}}</a></h1>
    <p>{{description}}</p>
  </section>
{{/each}}
